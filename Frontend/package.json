{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --port 5000",
    "start": "sirv public --single",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "e2e": "cypress open",
    "ci:start-server": "http-server --path ./src -p 4200",
    "cy:run": "cypress run --headless --browser chrome",
    "ci:cy-run": "start-server-and-test ci:start-server http://localhost:4200 cy:run"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.56",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.3.7",
    "chance": "^1.1.8",
    "cypress": "^9.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "firebase": "^9.2.0",
    "http-server": "^14.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "start-server-and-test": "^1.14.0",
    "svelte": "^3.42.6",
    "svelte-barcode": "^1.1.0",
    "svelte-check": "^2.2.6",
    "svelte-forms": "^1.1.51",
    "svelte-lazy": "^1.0.12",
    "svelte-preprocess": "^4.9.4",
    "tailwindcss": "^2.2.17",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "dependencies": {
    "@tailwindcss/typography": "^0.5.0",
    "build": "^0.1.4",
    "sirv-cli": "^1.0.14",
    "svelte-i18n": "^3.3.13",
    "svelte-qrcode": "^1.0.0",
    "vite": "^2.7.0"
  }
}
